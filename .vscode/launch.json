  "version": "0.2.0",
  "configurations": [
    {
      "name": "Linux",
      "type": "lldb",
      "request": "launch",
      "program": "build/htmlrc",
      "args": ["examples/single-page/index.html", "examples/single-page/out"]
    },
    {
      "name": "Windows",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/htmlrc.exe",
      "args": [
        "examples/single-page-routing/index.html",
        "examples/single-page-routing/out"
      ],
      "cwd": "${workspaceRoot}"
    }
  ]
}
